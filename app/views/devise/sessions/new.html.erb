  <div class="call-to-action clearfix" id="join">
    <div class="col-md-12">
      <i class="fa fa-users" aria-hidden="true"></i>
      <p>
        <span>Add your photo and contribute!
        </span><br>
        <a href="/about">This project is an open invitation to all photographers in Miami!</a>
      </p>
      <div class="cta-btns">
        <a target="_blank" href="/signup" class="btn btn-pink btn-lg flex-item">Sign Up</a>
      </div>
    </div>
  </div>

  <!--Footer Links-->
  <div class="container-fluid">
    <div class="row">

      <!--FOOTER ************************************************************************************************************************-->

    </div>
  </div>
